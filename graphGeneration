# Enters R terminal
R
# Used for fancy plots
library(ggplot2)
# Reads in with header names
report <- read.csv('report.csv')

scatterPlot <- ggplot(report, aes(domain, reformulation_time)) + geom_point(aes(color=algorithm))
boxPlot1 <- ggplot(report, aes(domain, reformulation_time)) + geom_boxplot()
boxPlot2 <- ggplot(report, aes(algorithm, reformulation_time)) + geom_boxplot()
ggsave(plot=scatterPlot, filename="scatterPlot.png", width=16, height=16)
ggsave(plot=boxPlot1, filename="boxPlot1.png", width=16, height=16)
ggsave(plot=boxPlot2, filename="boxPlot2.png", width=16, height=16)
